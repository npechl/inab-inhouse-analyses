# Load the required library
library(randomForest)
# Example: Simulated gene counts matrix (120 samples x 2,000 genes)
set.seed(42)  # For reproducibility
gene_counts <- matrix(rnorm(120 * 2000), nrow = 120, ncol = 2000)  # 120 samples, 2,000 genes
# Labels for the samples (5 cancer types, 20 samples per type, and 20 control samples)
labels <- factor(c(rep("Breast", 20), rep("Lung", 20), rep("Colon", 20),
rep("Liver", 20), rep("Prostate", 20),
rep("Control", 20)))  # 20 control samples
# Train a Random Forest classifier
rf_model <- randomForest(x = gene_counts, y = labels, importance = TRUE)
# Extract feature importance
importance_scores <- importance(rf_model)
# Sort genes by importance and select the top 50 genes
top_genes <- rownames(importance_scores[order(-importance_scores[,1]), ])[1:50]
# Output: Top 50 genes
print(top_genes)
